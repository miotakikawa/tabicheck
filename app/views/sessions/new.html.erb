<h2> ログイン</h2><br>
<table>
<%= form_with model: @user, url: users_create_path, local: true do |f| %>
<tr>
 <td><%= f.label :email, "メールアドレス" %></td>
 <td><%= f.text_field :email %></td>
</tr>
<tr>
 <td><%= f.label :password, "パスワード" %></td>
 <td><%= f.password_field :password %></td>
</tr>
</table>
<div>
 <%= f.submit "ログイン", class:"btn btn-success" %>
</div>
<% end %>

