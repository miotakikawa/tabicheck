<h1> ログイン</h1>
<div>
<%= form_with model: @user, url: login_path, local: true do |f| %>
<div>
 <%= f.label :email, "メールアドレス" %>
 <%= f.text_field :email %>
</div>
<div>
 <%= f.label :password, "パスワード" %>
 <%= f.password_field :password %>
</div>

<div>
 <%= f.submit "ログイン", class:"btn btn-success" %>
</div>
<% end %>

</div>