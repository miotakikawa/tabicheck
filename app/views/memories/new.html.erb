<div class="container">
<div class="alert alert-dark col-5 d-flex" role="alert">
<strong>思い出投稿</strong>
</div>
</div>
<div class="container">
    <%= form_with model: @memory, url:memories_path, local:true do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title %><br><br>
    <h4></h4>
    <%= f.attachment_field :image %><br><br>
    <h4>Comment</h4>
    <%= f.text_area :body %><br><br><br>
    <div>
    <%= f.submit 'Create Memory', class: "btn btn-success" %>
    </div>
    <% end %>
</div>
